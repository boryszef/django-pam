[run]
source = django_pam
#branch = True
omit =
     ../*migrations*
     *urls*
     *tests*

[report]
show_missing = True
skip_covered = True
precision = 2

# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    if log\.is*

[html]
title = Django PAM Coverage Report
directory = docs/htmlcov
